<%# Display post, provide edit and delete options %>
<div class="container">
  <div class="py-5">
    <h2>Edit Article</h2>
    <%= simple_form_for(@article) do |f| %>
      <%= f.input :title %>
      <%= f.input :content %>
      <%= f.submit class: 'btn btn-primary'%>
    <% end %>
  </div>
</div>
